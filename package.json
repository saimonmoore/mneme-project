{
  "name": "mneme",
  "private": true,
  "version": "0.0.0",
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/desktop",
      "apps/mobile-proxy",
      "apps/p2p-proxy",
      "apps/web"
    ]
  },
  "license": "MIT",
  "scripts": {
    "turbo": "turbo",
