{
  "name": "mneme",
  "main": "dist/src/index.js",
  "version": "0.0.1",
  "type": "module",
  "pear": {
    "name": "mneme",
    "type": "terminal"
  },
  "license": "Apache-2.0",
  "scripts": {
    "switch-to-node": "./scripts/switch-interpreter.sh node && rm -fr node_modules && rm -fr ../../node_modules && yarn install",
    "dev-pear": "./scripts/proxy-root-node-modules.sh pear dev .",
    "pear1": "clear; rm -fr data; yarn run build && yarn run dev-pear",
    "pear2": "clear; rm -fr data2; yarn run dev-pear -- -s 2 -b ",
    "stage:dev": "pear stage --ignore src,test,.turbo dev",
    "stage:prod": "pear stage --ignore src,test,.turbo production",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch --clear-screen=false src/index.ts",
    "build": "rimraf dist && tsc --build --verbose && tsc-alias",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "check:types": "tsc --noEmit",
    "server": "./scripts/proxy-root-node-modules.sh bare server.js",
    "test": "NODE_ENV=test jest",
    "test:jest:strict": "NODE_ENV=test jest --detect-open-handles",
    "test:jest:watch": "NODE_ENV=test jest --watch",
    "test:brittle": "NODE_ENV=test brittle __tests__/e2e.brittle.test.js",
    "test:brittle:watch": "nodemon --watch . --exec 'npm run test:brittle:all'",
    "test:e2e": "NODE_ENV=test brittle __tests__/all.brittle.js",
    "test:brittle:generate": "brittle -r __tests__/all.brittle.js ./**/*.brittle.test.js"
  },
  "dependencies": {
    "@mneme/domain": "*",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "assert": "npm:bare-node-assert@^1.0.0",
    "autobase": "6.0.28",
    "b4a": "^1.6.6",
    "bare-assert": "^1.0.1",
    "bare-buffer": "^2.3.0",
    "bare-console": "^5.1.0",
    "bare-crypto": "^1.0.0",
    "bare-events": "^2.2.2",
    "bare-fs": "^2.3.0",
    "bare-http1": "^3.2.1",
    "bare-https": "^1.0.0",
    "bare-inspector": "^1.2.1",
    "bare-module": "^3.2.0",
    "bare-os": "^2.3.0",
    "bare-path": "^2.1.2",
    "bare-process": "^1.3.2",
    "bare-readline": "^1.0.3",
    "bare-repl": "^2.0.1",
    "bare-stream": "^1.0.0",
    "bare-subprocess": "^3.0.0",
    "bare-timers": "^2.0.9",
    "bare-tls": "^1.0.0",
    "bare-tty": "^3.3.1",
    "bare-url": "^1.1.0",
    "bare-utils": "^1.0.0",
    "bare-worker": "^1.0.0",
    "bare-zlib": "^1.0.1",
    "buffer": "npm:bare-node-buffer@^1.0.0",
    "console": "npm:bare-node-console@^1.0.1",
    "corestore": "^6.18.1",
    "crypto": "npm:bare-node-crypto@^1.0.0",
    "events": "npm:bare-node-events@^1.0.1",
    "express": "^4.19.2",
    "fs": "npm:bare-node-fs@^1.0.2",
    "graceful-goodbye": "^1.3.0",
    "http": "npm:bare-node-http@^1.0.1",
    "https": "npm:bare-node-https@^1.0.0",
    "hyperbee": "^2.19.0",
    "hyperdht": "^6.14.0",
    "hyperswarm": "^4.7.14",
    "inspector": "npm:bare-node-inspector@^1.0.1",
    "istextorbinary": "^9.5.0",
    "jsonschema": "^1.4.1",
    "lexicographic-integer": "^1.1.0",
    "minimist": "^1.2.8",
    "module": "npm:bare-node-module@^1.0.0",
    "os": "npm:bare-node-os@^1.0.1",
    "path": "npm:bare-node-path@^1.0.1",
    "pear-inspect": "^1.0.8",
    "process": "npm:bare-node-process@^1.0.1",
    "random-access-memory": "^6.2.1",
    "readline": "npm:bare-node-readline@^1.0.1",
    "reflect-metadata": "^0.1.13",
    "stream": "npm:bare-node-stream@^1.0.0",
    "timers": "npm:bare-node-timers@^1.0.0",
    "tls": "npm:bare-node-tls@^1.0.0",
    "tty": "npm:bare-node-tty@^1.0.1",
    "url": "npm:bare-node-url@^1.0.1",
    "util": "npm:bare-node-util@^1.0.0",
    "worker_threads": "npm:bare-node-worker-threads@^1.0.0",
    "zlib": "npm:bare-node-zlib@^1.0.0",
    "zod": "^3.22.5"
  },
  "imports": {
    "readline": {
      "bare": "bare-readline",
      "default": "readline"
    },
    "readline/*": {
      "bare": "bare-readline/*",
      "default": "readline/*"
    },
    "crypto": {
      "bare": "bare-crypto",
      "default": "crypto"
    },
    "crypto/*": {
      "bare": "bare-crypto/*",
      "default": "crypto/*"
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/minimist": "^1.2.5",
    "brittle": "^3.0.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier-config": "*",
    "rimraf": "^3.0.2",
    "tsconfig": "*"
  }
}
