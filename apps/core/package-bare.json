    "@types/node": "^18.15.11",
    "autobase": "latest",
    "b4a": "^1.6.6",
    "bare-console": "^4.1.0",
    "bare-events": "^2.2.0",
    "bare-fs": "^2.1.5",
    "bare-http1": "^2.0.3",
    "bare-inspector": "^1.1.2",
    "bare-os": "^2.2.0",
    "bare-path": "^2.1.0",
    "bare-process": "^1.3.0",
    "bare-readline": "^1.0.0",
    "bare-repl": "^1.0.3",
    "bare-subprocess": "^2.0.4",
